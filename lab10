#include <stdio.h>
#include <string.h>

// Задание 1

int main()
{
    char str[21];
    int i;
    printf("Vvedite stroky\n");
    scanf("%s20", str);
    for (i = 0; i <= strlen(str); i++)
    {
        if (str[i] == 'a') str[i] = 'b';
        if (str[i] == 'A') str[i] = 'B';
            
    }
    printf("%s\n", str);
    
    return 0;
}
